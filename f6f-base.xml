<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <sim include="Nasal/views.xml">

    <status>devel</status>
    <author>BARANGER Emmanuel</author>

    <fuel-fraction>1.0</fuel-fraction>

    <sound>
      <audible>true</audible>
      <path>Aircraft/F6F-Hellcat/Sounds/f6f-sound.xml</path>
    </sound>

    <model>
      <path>Aircraft/F6F-Hellcat/Models/f6f.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <submodels> 
      <serviceable type="bool">true</serviceable>
      <path>Aircraft/F6F-Hellcat/Models/Effects/submodels.xml</path>
    </submodels>

    <startup>
      <splash-texture>Aircraft/F6F-Hellcat/f6f-splash.png</splash-texture>
    </startup>

    <multiplay>
      <chat_display>1</chat_display>
      <generic>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
        <int type="int">0</int>
      </generic>
    </multiplay>

    <help>
      <title>GRUMANN TBM f6f</title>
      <key>
        <name>d</name>
        <desc>Open/Close canpy</desc>
      </key>
      <key>
        <name>e</name>
        <desc>Guns fire</desc>
      </key>
      <key>
        <name>E</name>
        <desc>Open/Close wings</desc>
      </key>
    </help>

    <menubar include="Dialogs/f6f-menu.xml"/>

  </sim>

  <engines>
    <engine n="0">
      <engine-num type="int">0</engine-num>
      <running>true</running>
    </engine>
    <engine n="1">
      <engine-num type="int">1</engine-num>
      <running>true</running>
    </engine>
  </engines>

  <consumables>
    <fuel>
      <tank n="0">
        <tank-num type="int">0</tank-num>
        <level-gal_us archive="y">2070</level-gal_us>
      </tank>
    </fuel>
  </consumables>

  <input>
    <keyboard include="Nasal/f6f-keyboard.xml"/>
  </input>

  <nasal>
    <f6f>
      <file>Aircraft/F6F-Hellcat/Nasal/doors.nas</file>
      <file>Aircraft/F6F-Hellcat/Nasal/liveries.nas</file>
      <file>Aircraft/F6F-Hellcat/Nasal/light.nas</file>
    </f6f>
    <guns>
      <file>Aircraft/F6F-Hellcat/Nasal/weapons.nas</file>
      <file>Aircraft/F6F-Hellcat/Nasal/revi.nas</file>
    </guns>
  </nasal>

</PropertyList>

